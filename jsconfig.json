{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["DOM", "DOM.Iterable", "WebWorker", "ESNext.Array", "DOM", "ESNext"],
		"types": ["vite/client"],
		"moduleResolution": "bundler",
		"module": "es2022",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2022"
	}
}
